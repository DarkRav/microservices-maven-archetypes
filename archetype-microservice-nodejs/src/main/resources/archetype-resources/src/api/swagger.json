{
  "swagger": "2.0",
  "info": {
    "description": "Node microservices ${microservice-name}",
    "version": "1.0.0",
    "title": "Swagger node microservice ${microservice-name}",
    "termsOfService": "http://swagger.io/terms/",
    "contact": {
      "email": "rafabc1979@gmail.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  
  "basePath": "/",
  "tags": [
    {
      "name": "${microservice-name}",
      "description": "Microservice for ${microservice-name}",
      "externalDocs": {
        "description": "Find out more",
        "url": "http://swagger.io"
      }
    }
  ],
  "schemes": [
    "http"
  ],
  "paths": {
    "/${request-mapping}": {
      "get": {
        "tags": [
          "micro"
        ],
        "summary": "${microservice-name}",
        "description": "detail for ${microservice-name} in node bak",
        "operationId": "${request-mapping}",
        "produces": [
          "application/json"
        ],
        "parameters": [

        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "object",
              "items": {
                "$ref": "#/definitions/object"
              }
            }
          },
          "400": {
            "description": "Invalid status value"
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "micro_auth": {
      "type": "oauth2",
      "authorizationUrl": "http://micros.swagger.io/oauth/dialog",
      "flow": "implicit",
      "scopes": {
        "write:micros": "modify micro in your account",
        "read:micros": "read your micros"
      }
    },
    "api_key": {
      "type": "apiKey",
      "name": "api_key",
      "in": "header"
    }
  },
  "definitions": {
    "object": {
      "type": "object",
      "properties": {
        "field1": {
          "type": "string",
          "description": "field1"
        },
        "field2": {
          "type": "string",
          "description": "field2"
        }
      },
      "xml": {
        "name": "Order"
      }
    }
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  }
}